(defaction (target ((pose pose)))
    (to-list () `(target :pose ,pose :pre ,(synth to-list precond) :post ,(synth to-list postcond)))
  (to-req () (hcat (text "effettua una transizione verso ") (synth to-url pose)))
  (to-html () (span nil (text "effettua una transizione verso ") (synth to-url pose)
                    (maybes (list precond (span nil (text "Precondizione:")))
                            (list postcond (span nil (text "Postcondizione:")))))))
